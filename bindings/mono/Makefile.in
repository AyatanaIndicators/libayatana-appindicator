	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(assemblydir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(assemblydir)" || exit $$?; \
	done

